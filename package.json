{
  "name": "project2",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "ng": "ng",
    "build:backend": "tsc --project src/tsconfig.server.json",
    "start:backend": "npm run build:backend && node -r dotenv/config dist/server/server.js",
    "start:frontend": "ng serve --port 4200",
